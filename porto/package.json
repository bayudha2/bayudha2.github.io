        "@headlessui/react": "^1.4.2",
        "@reduxjs/toolkit": "^1.6.2",
        "@splidejs/react-splide": "^0.6.9",
        "@testing-library/jest-dom": "^5.15.0",
        "@testing-library/react": "^11.2.7",
        "@testing-library/user-event": "^12.8.3",
        "chokidar": "^3.5.2",
        "gh-pages": "^3.2.3",
        "postcss-import": "^14.0.2",
        "react": "^17.0.2",
        "react-cookies": "^0.1.1",
        "react-data-export": "^0.6.0",
        "react-dates": "^21.8.0",
        "react-dom": "^17.0.2",
        "react-hook-form": "^6.8.2",
        "react-redux": "^7.2.6",
        "react-router": "^6.0.2",
        "react-router-dom": "^6.0.2",
        "react-router-modal-gallery": "^1.1.3",
        "react-scripts": "4.0.3",
        "react-toastify": "^6.2.0",
        "react-use-keypress": "^1.3.0",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "swiper": "^6.8.4",
        "tailwind": "^4.0.0",
        "web-vitals": "^1.1.2",
        "xlsx": "^0.16.8",
        "yarn": "^1.22.17",
        "yup": "^0.29.3"
    },
    "scripts": {
        "build:tailwind": "postcss src/styles/index.css -o src/styles.output.css",
        "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/styles.output.css -c 'npm run build:tailwind'",
        "start:react": "react-scripts start",
        "prebuild": "NODE_ENV=production run-s build:tailwind",
        "start": "npm-run-all build:tailwind --parallel watch:tailwind start:react",
        "build": "react-scripts build",
        "predeploy": "npm run build",
        "deploy": "gh-pages -b master -d build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "homepage": "https://bayudha2.github.io/",
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "autoprefixer": "^9.8.8",
        "chokidar-cli": "^2.1.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.3.11",
        "postcss-cli": "^9.0.2",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17"
    }
}
